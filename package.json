{
  "name": "turborepo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:docs": "turbo dev --filter=docs",
    "dev:web": "turbo dev --filter=web",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "check-types": "turbo run check-types",
    "db:push": "cd ./packages/singlestore && npm run push && cd ../postgres && npm run push",
    "data:generate": "cd ./apps/data-generator && npm run generate:start",
    "data:load": "cd ./apps/data-generator && npm run load:start"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "prettier": "^3.5.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "turbo": "^2.4.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.2.3",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
